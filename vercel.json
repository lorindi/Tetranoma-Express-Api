{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app.js"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "JWT_SECRET_KEY": "@jwt_secret_key"
    }
  }